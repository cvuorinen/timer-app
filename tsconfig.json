{
  "extends": "./node_modules/electron-webpack/tsconfig-base.json",

  "compilerOptions": {
    "jsx": "react",
    "experimentalDecorators": true,

    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "strictNullChecks": true
  }
}
